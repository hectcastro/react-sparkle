{
  "name": "react-sparkle",
  "version": "1.0.1",
  "description": "A React component to increase the number of sparkles in your app",
  "homepage": "https://github.com/kmjennison/react-sparkle",
  "bugs": "https://github.com/kmjennison/react-sparkle/issues",
  "main": "build/index.js",
  "scripts": {
    "test": "yarn run lint && jest --env=jsdom",
    "start": "webpack --watch",
    "build": "NODE_ENV=production webpack",
    "lint": "standard"
  },
  "keywords": [
    "react",
    "sparkle",
    "sparkles",
    "react-component",
    "glitter",
    "canvas"
  ],
  "author": "Kevin Jennison",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-jest": "^23.0.0-alpha.0",
    "babel-loader": "^7.1.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^22.4.2",
    "prop-types": "^15.6.1",
    "react-dom": "^16.2.0",
    "standard": "^11.0.1",
    "webpack": "^4.1.1",
    "webpack-cli": "^2.0.12"
  },
  "dependencies": {
    "react": "^16.2.0",
    "resize-observer-polyfill": "^1.5.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "testEnvironment": "node",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  }
}
